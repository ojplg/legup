
#parse( "/templates/insert_bill_identification.vtl" )

<br/>

<table class="pure-table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Chamber</th>
            <th>Action Type</th>
            <th>Raw Data</th>
        </tr>
    </thead>

    <tbody>
        #foreach ( $date in $billHistory.actionDates )
            #foreach ( $action in ${billHistory.getActionsOnDate($date)} )
                <tr>
                    #if ( $foreach.first )
                        <td class="overlined">$date</td>
                    #else
                        <td></td>
                    #end
                    <td>$action.Chamber</td>
                    <td>$action.billActionType.Code</td>
                    <td>$action.rawActionData</td>
                </tr>
            #end
        #end
    </tbody>
</table>
